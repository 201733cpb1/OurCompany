<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid android:color="#FFFFFF" />
            <stroke android:width="3dp" android:color="#e7510b" />
            <corners android:bottomLeftRadius="12dp" android:bottomRightRadius="12dp" android:topLeftRadius="12dp" android:topRightRadius="12dp" />
        </shape>
    </item>
    <item android:state_pressed="false" >
        <shape>
            <solid android:color="#FFFFFF" />
            <stroke android:width="3dp" android:color="#649bf4" />
            <corners android:bottomLeftRadius="12dp" android:bottomRightRadius="12dp" android:topLeftRadius="12dp" android:topRightRadius="12dp" />
        </shape>
    </item>
</selector>
