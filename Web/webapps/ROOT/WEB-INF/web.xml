		<servlet-name>check-userid-servlet</servlet-name>
		<url-pattern>/Servlet/overlapCheck</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>user-join-servlet</servlet-name>
		<servlet-class>Servlet.User.JoinServlet</servlet-class>
