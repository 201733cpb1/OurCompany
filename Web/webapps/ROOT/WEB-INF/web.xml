		<servlet-class>Servlet.Animal.DeleteAnimalVaccine</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>delete-vaccine-servlet</servlet-name>
		<url-pattern>/Servlet/deleteVaccine</url-pattern>
	</servlet-mapping>
	
